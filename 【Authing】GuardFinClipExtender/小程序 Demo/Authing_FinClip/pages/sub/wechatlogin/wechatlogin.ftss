.container {
    padding: 10px 20px;
    align-content: center;
  }
  .content {
    margin: 30px 0px;
    padding: 10px;
    border-bottom: 2rpx solid #F5F5F5;
    background-color: #FFFFFF;
    align-items: center;
    justify-content: space-between;
  }
  .itemView {
    width: 100%;
    right: 30px;
    display: flex;
    margin: 2px;
    justify-content: flex-end;
    white-space: pre-line;
    word-break:break-all;
  }
  .itemText {
    font-size: 35rpx;
  }
  
  