@keyframes boxtp{
0% { transform: rotate(0deg); left: 75rpx; top: 0rpx; transform-origin:100% 100%}
100%{ transform: rotate(20deg);left: 75rpx; top: 0rpx; transform-origin:100% 100%}
}

@keyframes fly{
0% {  left: 75rpx; top: 0rpx; transform-origin:100% 100%}
100%{ tleft: 750rpx; top: -350rpx; transform-origin:100% 100%}
}

@keyframes money1{
0% { left:85rpx; top: 50rpx;  transform: rotate(5deg);   visibility: visible;}
10% {left:80rpx; top: 45rpx;  transform: rotate(5deg);   visibility: visible;}
20%{ left: 75rpx; top: 40rpx;  transform: rotate(10deg);   visibility: visible;}
30% { left:70rpx; top: 45rpx;  transform: rotate(5deg);   visibility: visible;}
40%{ left: 65rpx; top: 50rpx;  transform: rotate(10deg);   visibility: visible;}
50% { left:65rpx; top: 70rpx;  transform: rotate(5deg);   visibility: visible;}
60%{ left: 65rpx; top: 90rpx;  transform: rotate(10deg);   visibility: visible;}
70% { left:65rpx; top: 110rpx;  transform: rotate(5deg);   visibility: visible;}
80%{ left: 60rpx; top: 130rpx;  transform: rotate(10deg);   visibility: visible;}
90% { left:65rpx; top: 150rpx;  transform: rotate(5deg);   visibility: visible;}
100%{ left: 65rpx; top: 170rpx;  transform: rotate(10deg);   visibility: visible;}
}


@keyframes yao{
0% { transform: rotate(0deg); transform-origin: 50% 50%; }
5% { transform: rotate(15deg); transform-origin: 50% 50%; }

15% { transform: rotate(-15deg); transform-origin: 50% 50%; }

25% { transform: rotate(10deg); transform-origin: 50% 50%; }

35% { transform: rotate(-10deg); transform-origin: 50% 50%; }

45% { transform: rotate(7deg); transform-origin: 50% 50%; }

55% { transform: rotate(-7deg); transform-origin: 50% 50%; }

65% { transform: rotate(5deg); transform-origin: 50% 50%; }

75% { transform: rotate(-5deg); transform-origin: 50% 50%; }

85% { transform: rotate(3deg); transform-origin: 50% 50%; }

95%{ transform: rotate(-3deg); transform-origin: 50% 50%;}
100%{ transform: rotate(0deg); transform-origin: 50% 50%;}

}
@keyframes small_big{
0% { transform:scale(0); visibility: visible;} 
70% { transform:scale(1.2); visibility: visible;} 
85%{ transform:scale(0.9); visibility: visible;}
100%{ transform:scale(1); visibility: visible;}

}

@keyframes prizelight{
 0% { transform:rotate(0deg); opacity: 1; visibility: visible;} 
100%{  transform:rotate(360deg); opacity: 1;  visibility: visible;}
}

