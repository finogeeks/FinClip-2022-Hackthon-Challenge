.clearfix:after {
  visibility: hidden;
  display: block;
  font-size: 0;
  content: " ";
  clear: both;
  height: 0;
}
.navbar{
display: block;
position: fixed;
top: 0;
left: 0;
right: 0;
height: 100rpx;
border-bottom:1rpx solid #f5f5f5;
color: #333333;
font-size: 38rpx;
line-height: 100rpx;
z-index: 100;
text-align: center;
font-weight: bold;
background: #FFFFFF;
box-sizing: border-box;
}
.navbar .icon{
width: 45rpx;
height: 45rpx;
line-height: 45rpx;
position: absolute;
padding:27.5rpx 20rpx;
left: 0rpx;
}
.navbar .icon image{
width: 45rpx;
height: 45rpx;
vertical-align: middle;
}
.navbar .title{
font-size: 36rpx;
font-weight: normal;
color: #000000;
}

.tab-bar {
height: 100rpx;
line-height: 100rpx;
width: 100%;
position: fixed;
left: 0;
bottom: 0;
background: #ffffff;
overflow: hidden;
z-index: 99;
border-top: 1rpx solid #f2f2f2;
}

.tab-bar-border {
background-color: rgba(0, 0, 0, 0.33);
position: absolute;
left: 0;
top: 0;
width: 100%;
height: 1px;
transform: scaleY(0.5);
}

.tab-bar-item {
height: 100rpx;
line-height: 0rpx;
text-align: center;
float: left;
width:33.33%;
}

.tab-bar-item cover-image {
 width: 41rpx;
height: 41rpx;
margin: 10rpx auto;
display: block;
}

.tab-bar-item cover-view {
width: 100%;
height: 39rpx;
line-height: 39rpx;
text-align: center;
font-size: 20rpx;
color: #333333;
}


