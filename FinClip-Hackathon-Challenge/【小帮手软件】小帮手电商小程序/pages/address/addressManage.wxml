<view class="{{['content','theme-'+theme]}}"><link-address class="vue-ref" vue-id="0e0a4db1-1" height="{{height}}" data-ref="linkAddress" data-event-opts="{{[['^confirmCallback',[['confirmCallback']]]]}}" bind:confirmCallback="__e" bind:__l="__l"></link-address><view class="row b-b"><text class="tit">联系人</text><input class="input" type="text" placeholder="收货人姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['addressData']]]]]}}" value="{{addressData.name}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">手机号</text><input class="input" type="number" placeholder="收货人手机号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','mobile','$event',[]],['addressData']]]]]}}" value="{{addressData.mobile}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">地区</text><text data-event-opts="{{[['tap',[['popup_district',['$event']]]]]}}" class="input" bindtap="__e">{{''+addressData.area+''}}</text></view><view class="row b-b"><text class="tit">地址</text><input class="input" type="text" placeholder="收货详细地址" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','address','$event',[]],['addressData']]]]]}}" value="{{addressData.address}}" bindinput="__e"/></view><view class="row default-row"><text class="tit">设为默认</text><switch checked="{{addressData.is_default}}" color="{{themeBaseColor}}" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">提交</button><copyright vue-id="0e0a4db1-2" bind:__l="__l"></copyright></view>