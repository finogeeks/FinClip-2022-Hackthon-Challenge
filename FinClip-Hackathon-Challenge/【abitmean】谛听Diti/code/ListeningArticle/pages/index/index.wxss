/**index.wxss**/
.theme-color {
  color: var(--themeColor);
}

@keyframes fadeIn {
  0% {
    opacity: 0;
    /*初始状态 透明度为0*/
  }

  50% {
    opacity: 0;
    /*中间状态 透明度为0*/
  }

  100% {
    opacity: 1;
    /*结尾状态 透明度为1*/
  }
}

.la-search-bar {
  padding-top: 50%;
  animation-name: fadeIn;
  /*动画名称*/
  animation-duration: 2s;
  /*动画持续时间*/
  animation-iteration-count: 1;
  /*动画次数*/
  animation-delay: 0s;
  /*延迟时间*/
}

.la-search-bar-more {
  /* transform: translateY(-50%); */
  animation-name: fadeIn;
  /*动画名称*/
  animation-duration: 2s;
  /*动画持续时间*/
  animation-iteration-count: 1;
  /*动画次数*/
  animation-delay: 0s;
  /*延迟时间*/
}

.search-button {
  background-color: var(--themeColor);
  position: absolute;
  top: 0px;
  right: 0px;
  box-sizing: border-box;
  border-radius: 0px 4px 4px 0;
  width: 60px;
  height: 100%;
  line-height: 100%;
  color: #ffffff;
  z-index: 10;
}

.search-button-text {
  position: absolute;
  top: 12px;
  right: 14px;
  z-index: 1;
}

.la-more {
  background-color: var(--themeColor);
  transform: rotate(90deg);
  margin: auto;
  display: flex;
  animation-name: fadeIn;
  /*动画名称*/
  animation-duration: 2s;
  /*动画持续时间*/
  animation-iteration-count: 999;
  /*动画次数*/
  animation-delay: 0s;
  /*延迟时间*/
}

.la-more-hide{
  display: none;
}

