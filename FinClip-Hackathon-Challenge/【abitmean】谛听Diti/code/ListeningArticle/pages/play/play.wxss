}

.background_img {
  width: 100%;
  height: 100%;
  filter: blur(20px);
  z-index: -2;
  transform: scale(1.5);
}

.background_img,.mask {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
}

.mask {
  right: 0;
  background-color: rgba(0,0,0,.5);
  z-index: -1;
}

.la-brief {
  width: 80%;
  height: 100rpx;
  margin: 5rem auto 0 auto;
}

.la-name {
  font-size: 30rpx;
  color: #fff;
  width: 100%;
  text-align: center;
  display: -webkit-box;
  word-break: break-all;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  text-decoration: underline;
}

.la-cover-show {
  position: relative;
  width: 100%;
  height: 420rpx;
  display: flex;
  justify-content: center;
  box-sizing: border-box;
}

.coverImg {
  width: 340rpx;
  height: 340rpx;
  border-radius: 50%;
  border: 3px solid #fff;
  animation: rotate 15s linear infinite;
  animation-play-state: paused;
}

.slider-view {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.audio-slider {
  width: 60%;
}

.audio-current-time,.audio-end-time {
  font-size: 28rpx;
  color: #fff;
  width: 10%;
}

.audio-current-time {
  text-align: right;
}

.audio-end-time {
  text-align: left;
}

.play_suspend {
  margin-bottom: 20rpx;
  height: 100rpx;
  width: 100%;
  text-align: center;
  line-height: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center
}

.play-view{
    margin-bottom: 60rpx;
    height: 100rpx;
    width: 100%;
    text-align: center;
    line-height: 100rpx;
}

.img_play_suspend {
  width: 80rpx;
  height: 80rpx;
}

.icon_last_play,.icon_next_play {
  width: 60rpx;
