}

.collect {
  color: #557cf5;
}

.cancel-collect {
  color: #fff;
}

.list-icon {
  font-size: 64rpx;
  color: #fff;
}

.share-button-view {
  margin-right: 42rpx;
  position: relative;
  display: flex;
  align-items: center;
}

.share-icon {
  font-size: 64rpx;
  line-height: 64rpx;
  color: #fff;
}

.share-button {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  opacity: 0;
}

.coverImg.play {
  animation: rotate 15s linear infinite;
  animation-fill-mode: forwards;
}

.moveCircle.play {
  animation: moveCircle 2.5s linear infinite;
  animation-fill-mode: forwards;
}

.hide {
  display: none;
}

@keyframes rotate {
  0% {
      transform: rotate(0);
  }

  100% {
      transform: rotate(1turn);
  }
}

.moveCircle {
  border-radius: 50%;
  border: 1px solid #fff;
  width: 360rpx;
  height: 360rpx;
  animation: moveCircle 2.5s linear infinite;
  animation-play-state: paused;
}

@keyframes moveCircle {
  0% {
      width: 360rpx;
      height: 360rpx;
      border: 1px solid #fff;
  }

  30% {
      width: 510rpx;
      height: 510rpx;
      border: 1px solid hsla(0,0%,100%,.8);
  }

  50% {
      width: 610rpx;
      height: 610rpx;
      border: 1px solid hsla(0,0%,100%,.6);
  }

  80% {
      width: 700rpx;
      height: 700rpx;
      border: 1px solid hsla(0,0%,100%,.4);
  }

  99% {
      width: 375px;
      height: 375px;
      border: 1px solid hsla(0,0%,100%,.1);
  }

  100% {
      width: 0;
      height: 0;
      border: 1px solid hsla(0,0%,100%,0);
  }
}

